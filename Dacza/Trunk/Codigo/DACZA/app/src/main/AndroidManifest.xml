<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duxstar.dacza"
    android:versionCode="1"
    android:versionName="1.0.0.0">

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="13" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <permission
        android:name="com.duxstar.dacza.vistas.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.duxstar.dacza.vistas.permission.MAPS_RECEIVE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!--<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <!-- <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />-->
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR" />
    <uses-permission android:name="android.permission.CAMERA" />

    <permission
        android:name="android.permission.CONTROL_LOCATION_UPDATES"
        android:protectionLevel="signatureOrSystem" />
    <permission
        android:name="android.permission.STATUS_BAR"
        android:protectionLevel="signature" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:noHistory="true"
        android:theme="@style/RouteLTheme">

        <activity
            android:name=".vistas.InicioDacza"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <!-- <category android:name="android.intent.category.HOME"/>  Para deshabilitar boton HOME (se quito por problemas con algunos celulares) -->
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.ServidorComunicaciones"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.ConfiguracionTerminal"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.AccesoSistema"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.SeleccionFecha"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.SeleccionActividades"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.ConsultaInventario"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.SeleccionOrden"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.CapturaOrden"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".vistas.CapturaArticuloOrden"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.ConsultaOrden"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.SeleccionRecarga"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.CapturaRecarga"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.SeleccionDevolucion"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.CapturaDevolucion"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.ConsultaTraspaso"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.ConsultaCliente"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name=".vistas.BuscaArticulo"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity
            android:name=".vistas.BuscaCliente"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable_cliente" />
        </activity>

        <activity
            android:name=".vistas.BuscaAgente"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable_agente" />
        </activity>

        <activity
            android:name=".vistas.BuscaVin"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTop"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable_vin" />
        </activity>

        <activity
            android:name=".vistas.CapturaImagen"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan"></activity>

        <provider
            android:name=".providers.BusquedaProvider"
            android:authorities="busqueda"
            android:configChanges="keyboardHidden|orientation"
            android:syncable="false">
        </provider>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>

</manifest>
