                        <div class="row form-group">
                            <div class="col-md-6">
                                <dir-pagination-controls max-size="8"
                                                         direction-links="true"
                                                         boundary-links="true">
                                </dir-pagination-controls>
                            </div>
                        </div>
                        <!--Buscar-->
                        <div class="input-group">
                            <span class="input-group-addon"><img width="16" height="16" src="../Content/images/buscar.png" /></span>
                            <input type="text" placeholder="{{translation.BTBuscar}}" class="form-control" ng-model="search" />
                        </div>
                    </div>*@
                    <br />
                    <div class="scrow" ng-mousedown="count = count + 1" ng-init="count=0">
                        <!--Tabla-->
                        <div class="col-md-12 form-group" style="padding-right:0px;">
                            <table class="table table-bordered table-historial">
                                <tr class="Lnegras">
                                    @*<td style="width:8%"></td>*@
