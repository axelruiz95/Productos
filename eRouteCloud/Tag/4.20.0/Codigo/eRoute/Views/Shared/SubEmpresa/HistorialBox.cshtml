<div id="HistorialBox" class="modal fade">
    <div class="modal-dialog-hist">
        <div  class="modal-content">
            <!-- dialog header -->
            <div class="modal-header">
                <button type="button" class="close modal-title" data-dismiss="modal" aria-label="Close" ng-click="CancelarHistBox()"><span style="color: yellow" aria-hidden="true">&times;</span></button>
                <h2 class="modal-title" id="lblMsgTitulo">{{translation.ERMCONHESC_HConfiguracion}}</h2>
                @*<label id="lblMsgMensaje" class="control-label text-label"></label>*@
