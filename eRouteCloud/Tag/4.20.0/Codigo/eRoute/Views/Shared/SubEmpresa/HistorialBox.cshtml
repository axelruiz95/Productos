                                    <td><input class="camposInput" type="checkbox" name="{{'SEMH.Comprobante' + $index }}" ng-checked="{{SEMH.Comprobante}}" disabled></td>
                                    @*<td>{{SEMH.Comprobante}}</td>*@
                                    <td><input class="camposInput" type="checkbox" name="{{'SEMH.FoliosFiscales' + $index }}" ng-checked="{{SEMH.FoliosFiscales}}" disabled></td>
                                    @*<td>{{SEMH.FoliosFiscales}}</td>*@
                                    <td>{{SEMH.DirReportesMensuales}}</td>
                                    <td>{{SEMH.FormatoFactura}}</td>   @*verificar*@
                                    <td>{{SEMH.ClienteGenerico}}</td>
                                    <td>{{SEMH.FormatoNC}}</td>   @*verificar*@
                                    <td>{{SEMH.VersionCFD}}</td>
                                    <td>{{SEMH.ProveedorTimbre}}</td> @*verificar*@
                                    <td>{{SEMH.CustomerID}}</td>
                                    <td>{{SEMH.CustomerKey}}</td>
                                    <td>{{SEMH.ServidorTimbre}}</td>
                                    <td>{{SEMH.ServidorCancelacion}}</td>
                                    <td>{{SEMH.ArchivoPFX64}}</td>
                                    <td>{{SEMH.CerFinKok}}</td>
                                    <td>{{SEMH.KeyFinKok}}</td>
                                    <td><input class="camposInput" type="checkbox" name="{{'SEMH.ReciboElecPago' + $index }}" ng-checked="{{SEMH.ReciboElecPago}}"disabled></td>
