<div id="ProdContenedor" class="modal fade">
    <div class="modal-dialog-contenedor">
        <div id="modal-content" class="modal-content">
            <!-- dialog header -->
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" @*ng-click="Cancelar()"*@ aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title" id="lblMsgTitulo">{{translation.ERMPROESC_IGeneral}}</h2>
            </div>
            <!-- dialog body -->
            <div class="modal-body-contenedor">
                <form role="form">
                    <div class="container">
                        <div class="form-group col-md-12" style="margin-top:-10px; margin-bottom: -5px">
                            @*<label id="lblMsgMensaje" class="control-label text-label"></label>*@
                            <div class="col-md-12" style="margin-left:0">
                                <dir-pagination-controls max-size="9"
                                                         direction-links="true"
                                                         boundary-links="true">
                                </dir-pagination-controls>
                            </div>
                        </div>
                        <!--Buscar-->
                        <div class="input-group">
                            <span class="input-group-addon"><img width="16" height="16" src="../Content/images/buscar.png" /></span>
                            <input type="text" placeholder="{{translation.BTBuscar}}" class="form-control" ng-model="searchContenedor" />
                        </div>
                    </div>
                    <div class="scrow">
                        @*<div style="margin: 0 auto; display: table;">
                            </div>*@
                        <!--Tabla-->
                        <div class="col-md-12 form-group tablaproductobox" style="padding-right:0px;">
                            <table class="table table-bordered">
                                <tr class="Lnegras" style="position: -webkit-sticky; position: sticky; top: 0 !important">
                                    <td></td>
                                    <td>{{translation.PROProductoClave}}</td>
                                    <td>{{translation.PROId}}</td>
                                    <td>{{translation.PROTipo}}</td>
                                    <td>{{translation.PRONombreCorto}}</td>
                                    <td>{{translation.PROTipoFase}}</td>
                                </tr>
                                <tr dir-paginate="c in cProContenedorList|orderBy:sortKey:reverse|filter:searchContenedor|itemsPerPage:contenedorPag">
                                    <td>
                                        <button ng-click="addContenedor(c.ProductoClave,c.Nombre)" data-dismiss="modal" class="btn btn-success btn-xs"><i class="fa fa-plus" aria-hidden="true"></i></button>
                                    </td>
                                    <td>{{c.ProductoClave}}</td>
                                    <td>{{c.Id}}</td>
                                    <td>{{c.Tipo}}</td>
                                    <td>{{c.Nombre}}</td>
                                    <td>{{c.TipoFase}}</td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </form>
            </div>
            <!-- dialog buttons -->
            <div class="modal-footer">
                <button type="button" style="margin-top:0;" data-dismiss="modal" ng-click="Cancelar()" class="btn btn-default">{{translation.BTCancelar}}</button>
            </div>
        </div>
    </div>
</div>
