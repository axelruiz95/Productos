<div id="RutaProductoBox" class="modal fade">
    <div class="modal-dialog" id="R">
        <div class="modal-content">

            <!-- dialog header -->
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" ng-click="Add=[]"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title">{{translation.ERMIMEESC_IGeneral}}</h2>
            </div>

            <div class="row" ng-show="!esquemaDisponible">
                <div class="col-md-12">
                    <div class="error-message error-esquema" ng-show="!esquemaDisponible">{{translation.E0694.replace('$0$',EsquemaIDPadreNombre).replace('$1$', translation.XEsquema).replace('$2$',NombreEstatusEsquema)}}</div>
                </div>
            </div>

            <!-- dialog body -->
            <div class="modal-body" ng-init="index = false">
                <tree-grid tree-data="tree_data"
                           col-defs="col_defs"
                           expand-on="expanding_property"
                           funct-del="ActionUrl"
                           tree-control="myTreeControl"
                           on-click="addProducto(branch['Clave'], branch['Nombre'], branch['EsquemaID']);">
                           @*on-click="addProducto(branch['Clave'], branch['Nombre'], branch['EsquemaID']); Inicializar();">*@
                </tree-grid>
            </div>

            <!-- dialog buttons -->
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="addEsquema();">{{translation.BTAceptar}}</button>
            </div>
        </div>
    </div>
</div>

