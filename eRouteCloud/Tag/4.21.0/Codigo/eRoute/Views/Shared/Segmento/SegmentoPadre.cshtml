<div id="SegmentoPadre" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <!-- dialog header -->
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title">{{translation.ERMIMEESC_IGeneral}}</h2>
            </div>

            <div class="row datos-esquema">
                <div class="col-md-6">
                    <div class="esquema-bloque">
                        <div class="row">
                            <div class="col-md-6">
                                <span><b>Nombre del esquema</b> :</span>
                            </div>
                            <div class="col-md-6">
                                <span> {{EsquemaIDPadreNombre}}</span>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <span><b>ID del Esquema</b> :</span>
                            </div>
                            <div class="col-md-6">
                                <span>{{EsquemaIDPadre}}</span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <span><b>Abreviatura</b> :</span>
                            </div>
                            <div class="col-md-6">
                                <span>{{AbreviaturaPadre}}</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                  
                </div>
            </div>
            <div class="row" ng-show="!esquemaDisponible">
                <div class="col-md-12">
                    <div class="error-message error-esquema" ng-show="!esquemaDisponible" >{{translation.E0694.replace('$0$',EsquemaIDPadreNombre).replace('$1$', translation.XEsquema).replace('$2$',NombreEstatusEsquema)}}</div>
                </div>
            </div>
            <!-- dialog body -->
            <div class="modal-body" ng-init="index = false">
                <tree-grid tree-data="tree_data"
                            col-defs="col_defs"
                            expand-on="expanding_property"
                                   funct-del="ActionUrl"
                            on-click="verificarEsquemasExistentes(branch.EsquemaID, branch.Nombre, branch.Tipo, branch.Abreviatura)">
                </tree-grid>
            </div>
            <!-- dialog buttons -->
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">{{translation.BTAceptar}}</button>
            </div>
        </div>
    </div>
</div>
