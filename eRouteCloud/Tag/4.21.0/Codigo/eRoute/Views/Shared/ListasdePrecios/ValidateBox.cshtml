<div id="ValidateBox" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <!-- dialog header -->
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h3 class="modal-title" id="lblYNMsgTitulo"></h3>
            </div>
            <!-- dialog body -->
            <div class="modal-body validateBox">
                <form role="form">
                    <div class="container">
                        <div class="form-group">
                            <label id="lblYNMsgMensaje" class="control-label text-label"></label>
                        </div>
                    </div>
                </form>
            </div>
            <!-- dialog buttons -->
            <div class="modal-footer">
                <button type="button" ng-click="ValidateYesNoMsgBox('YES')" class="btn btn-default" data-dismiss="modal">{{translation.BTAceptar}}</button>
                <button type="button" ng-click="ValidateYesNoMsgBox('NO')" class="btn btn-default" data-dismiss="modal">{{translation.BTCancelar}}</button>
            </div>
        </div>
    </div>
</div>
