<div id="VenListaPreciosBox" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <!-- dialog header -->
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title" id="lblMsgTitulo">Seleccionar Lista de Precios</h2>
            </div>

            <!-- dialog body -->
            <div class="scroll modal-body">
                <form role="form">
                    <div class="container">

                        <div class="row form-group">
                        <!--Buscar-->
                        <div class="col-md-12 ">
                            <input type="text" name="Buscar" ng-model="Buscar" class="form-control" placeholder="Buscar ..." />
                        </div>
                    </div>
                    <!--paginacion-->
                    <div class="row form-group">
                        <div class="col-md-12 ">
                            <dir-pagination-controls max-size="4"
                                                     direction-links="true"
                                                     boundary-links="true">
                            </dir-pagination-controls>
                        </div>
                    </div>

                        <!--Tabla-->
                        <div class="col-md-12 form-group" style="padding-right:0px;">
                            <table class="table table-bordered">
                                <tr class="Lnegras">
                                    <td>{{translation.XClave}}</td>
                                    <td>{{translation.ALMNombre}}</td>
                                    <td>{{translation.DECJerarquia}}</td>
                                    <td>{{translation.PRECFVTipo}}</td>
                                </tr>
                                <tr dir-paginate="r in cLista|orderBy:sortKey:reverse|filter:Buscar|itemsPerPage:tamPag" ng-click="SeleccionLi(r.PrecioClave,r.Nombre)">
                                    <td>{{r.PrecioClave}}</td>
                                    <td>{{r.Nombre}}</td>
                                    <td>{{r.Jerarquia}}</td>
                                    <td>{{r.CFVTipo}}</td>
                                </tr>
                            </table>
                        </div>
                    </div>
                </form>
            </div>
            <!-- dialog buttons -->
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">{{translation.BTAceptar}}</button>
            </div>
        </div>
    </div>
</div>
