        android:theme="@style/RouteLTheme" >
        <activity
            android:name=".vistas.InicioRouteLite"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <!-- <category android:name="android.intent.category.HOME"/>  Para deshabilitar boton HOME (se quito por problemas con algunos celulares) -->
            </intent-filter>
        </activity>
        <activity
            android:name=".vistas.ConfiguracionTerminal"
