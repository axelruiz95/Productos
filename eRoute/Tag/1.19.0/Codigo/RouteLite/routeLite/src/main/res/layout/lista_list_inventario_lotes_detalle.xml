<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="6" >

        <TextView
            android:id="@+id/lblItemInvenoryLote"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.7"
            android:gravity="right"
            android:text="TextView"
            android:textSize="@dimen/lista_list_inventario_lblsItems_textSize" />

        <TextView
            android:id="@+id/lblItemInvenoryCaducidad"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.1"
            android:gravity="right"
            android:text="TextView"
            android:textSize="@dimen/lista_list_inventario_lblsItems_textSize" />

        <TextView
            android:id="@+id/lblItemInventoryExistence"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="3dp"
            android:layout_weight="0.8"
            android:gravity="right"
            android:text="TextView"
            android:textSize="@dimen/lista_list_inventario_lblsItems_textSize" />

        <TextView
            android:id="@+id/lblItemInventoryAvalible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="3dp"
            android:layout_weight="0.8"
            android:gravity="right"
            android:text="TextView"
            android:textSize="@dimen/lista_list_inventario_lblsItems_textSize" />

        <TextView
            android:id="@+id/lblItemInventoryNoAvalible"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="3dp"
            android:layout_weight="0.8"
            android:gravity="right"
            android:text="TextView"
            android:textSize="@dimen/lista_list_inventario_lblsItems_textSize" />

        <TextView
            android:id="@+id/lblItemInventoryMalEstado"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="3dp"
            android:layout_weight="0.8"
            android:gravity="right"
            android:text="TextView"
            android:textSize="@dimen/lista_list_inventario_lblsItems_textSize" />

    </LinearLayout>

</LinearLayout>