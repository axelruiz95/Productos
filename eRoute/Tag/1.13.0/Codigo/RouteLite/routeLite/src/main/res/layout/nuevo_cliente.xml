                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:inputType="textNoSuggestions"
                            android:lines="1"
                            android:maxLength="40"
                            android:tag="ClienteDomicilioPE.Localidad"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblPaisPE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Pais"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <LinearLayout
                            android:id="@+id/linearLayout4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp" >

                            <EditText
                                android:id="@+id/txtPaisPE"
                                android:layout_width="@dimen/nuevo_cliente_txts_width"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:inputType="textNoSuggestions"
                                android:lines="1"
                                android:maxLength="32"
                                android:tag="ClienteDomicilioPE.Pais"
                                android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                            <ImageButton
                                android:id="@+id/btnDomicilioPEMas"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@null"
                                android:scaleType="centerInside"
                                android:src="@drawable/ic_menu_more" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layDomicilioPEMas"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/lblNumeroExtPE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Numero Exterior"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtNumeroExtPE"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:inputType="textNoSuggestions"
                            android:lines="1"
                            android:maxLength="16"
                            android:tag="ClienteDomicilioPE.Numero"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblNumeroIntPE"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Numero Interior"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtNumeroIntPE"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:inputType="textNoSuggestions"
                            android:lines="1"
                            android:maxLength="16"
                            android:tag="ClienteDomicilioPE.NumInt"
