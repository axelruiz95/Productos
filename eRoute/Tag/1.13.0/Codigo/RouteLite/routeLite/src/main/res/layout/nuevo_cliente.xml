                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:layout_weight="1"
                                android:background="@null"
                                android:src="@drawable/ic_menu_contraer" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
