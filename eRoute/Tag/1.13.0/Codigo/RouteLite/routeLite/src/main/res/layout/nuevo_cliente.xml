                            android:weightSum="2" >

                            <EditText
                                android:id="@+id/txtPaisDF"
                                android:layout_width="@dimen/nuevo_cliente_txts_width"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:inputType="textNoSuggestions"
                                android:lines="1"
                                android:maxLength="32"
