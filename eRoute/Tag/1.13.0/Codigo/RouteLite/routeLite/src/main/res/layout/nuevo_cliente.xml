                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@null"
                                android:src="@drawable/ic_menu_contraer" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layDomicilioFiscal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/layDomicilioDF"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:id="@+id/layFiscal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal"
                            android:weightSum="3" >

                            <TextView
                                android:id="@+id/lblDomicilioFiscal"
                                style="@style/MisClientes_NuevoCliente_DomicilioFiscal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Domicilio Fiscal"
                                android:textSize="@dimen/nuevo_cliente_lblDomFiscal_textSize" />

                            <ImageButton
                                android:id="@+id/imgCopyDFiscal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/ic_input_copy" />

                            <ImageButton
                                android:id="@+id/imgPasteDFiscal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:src="@drawable/ic_input_paste" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/lblCalleDF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Calle"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtCalleDF"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:inputType="textNoSuggestions"
                            android:lines="1"
                            android:maxLength="64"
                            android:tag="ClienteDomicilioDF.Calle"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblMunicipioDF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Municipio"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtMunicipioDF"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
