                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblPaisDF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Pais"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

