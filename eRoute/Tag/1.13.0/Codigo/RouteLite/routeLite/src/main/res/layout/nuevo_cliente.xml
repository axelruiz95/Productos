                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:text="Calle"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtCallePE"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:inputType="textNoSuggestions"
                            android:lines="1"
                            android:maxLength="64"
                            android:tag="ClienteDomicilioPE.Calle"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
