                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:inputType="phone"
                            android:digits="0123456789,#+*"
                            android:lines="1"
                            android:maxLength="64"
                            android:tag="Cliente.TelefonoContacto"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <CheckBox
                            android:id="@+id/chkExigirOrdenCompra"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:tag="Cliente.ExigirOrdenCompra"
                            android:text="Exigir Orden de Compra" />

                        <LinearLayout
