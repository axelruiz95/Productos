<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerInParent="true"
    android:layout_gravity="center"
    android:background="#FFFFFF"
    android:orientation="vertical"
    tools:context="com.amesol.routelite.vistas.AplicacionPromocion" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical" >

        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fillViewport="true"
            android:scrollbars="vertical" >

            <LinearLayout
                android:id="@+id/layCliente"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/layGenerales"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/lblDatosGenerales"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Datos Generales"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textSize="@dimen/nuevo_cliente_lblDatosGenerales_textSize" />

                    <TextView
                        android:id="@+id/lblClave"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Clave"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                    <EditText
                        android:id="@+id/txtClave"
                        android:layout_width="@dimen/nuevo_cliente_txts_width"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"
                        android:lines="1"
                        android:maxLength="10"
                        android:tag="Cliente.Clave"
                        android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                    <TextView
                        android:id="@+id/lblRazonSocial"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Razon Social"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                    <EditText
                        android:id="@+id/txtRazonSocial"
                        android:layout_width="@dimen/nuevo_cliente_txts_width"
                        android:layout_height="wrap_content"
                        android:inputType="textNoSuggestions"
                        android:lines="1"
                        android:maxLength="128"
                        android:tag="Cliente.RazonSocial"
                        android:textSize="@dimen/nuevo_cliente_txts_textSize" >

                        <requestFocus />
                    </EditText>

                    <TextView
                        android:id="@+id/lblRFC"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="RFC"
                        android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                    <LinearLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <EditText
                            android:id="@+id/txtRFC"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:inputType="textNoSuggestions"
                            android:lines="1"
                            android:maxLength="14"
                            android:tag="Cliente.IdFiscal"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <ImageButton
                            android:id="@+id/btnGeneralesMas"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@null"
                            android:scaleType="centerInside"
                            android:src="@drawable/ic_menu_more" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/layGeneralesMas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:scrollbars="vertical" >

                        <TextView
                            android:id="@+id/lblNombre"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nombre"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtNombre"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:inputType="textPersonName"
                            android:lines="1"
                            android:maxLength="32"
                            android:tag="Cliente.NombreCorto"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblCodigoBarras"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Codigo de Barras"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <EditText
                            android:id="@+id/txtCodigoBarras"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:inputType="textPersonName"
                            android:lines="1"
                            android:maxLength="64"
                            android:tag="Cliente.IdElectronico"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblFechaRegistro"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Fecha Registro"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblFechaRegistroCli"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:tag="Cliente.FechaRegistroSistema"
                            android:text="01/01/1900"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <TextView
                            android:id="@+id/lblFechaNacimiento"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Fecha de Nacimiento"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <TextView
                            android:id="@+id/lblFechaNacimientoCli"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="30dp"
                            android:layout_weight="0.32"
                            android:tag="Cliente.FechaNacimiento"
                            android:text="01/01/1900"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <TextView
                            android:id="@+id/lblContacto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Contacto"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
                            android:id="@+id/txtContacto"
                            android:layout_width="@dimen/nuevo_cliente_txts_width"
                            android:layout_height="wrap_content"
                            android:inputType="textPersonName"
                            android:lines="1"
                            android:maxLength="64"
                            android:tag="Cliente.NombreContacto"
                            android:textSize="@dimen/nuevo_cliente_txts_textSize" />

                        <TextView
                            android:id="@+id/lblTelefono"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Telefono"
                            android:textSize="@dimen/nuevo_cliente_lbls_textSize" />

                        <EditText
