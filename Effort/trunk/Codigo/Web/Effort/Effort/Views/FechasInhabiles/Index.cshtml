@model IEnumerable<Effort.Models.DiasNoHabiles>

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Main.cshtml";
}
<div class="container">
    <h2>Dias Inhabiles</h2>

    <a class="btn btn-primary" href="~/FechasInhabiles/Create">Crear Fecha Inhabil</a>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Descripcion</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in @Model)
            { 
                <tr>
                    <td><a href='~/FechasInhabiles/Edit?id=@item.id'>@item.id </a></td>
                    <td>@item.fecha</td>
                    <td>@item.descripcion</td>
                </tr>
            }

        </tbody>
    </table>
</div>

